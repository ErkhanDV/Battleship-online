{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "arrowParens": "always",
  "endOfLine": "auto",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "htmlvar(--text-color)spaceSensitivity": "css",
  "insertPragma": false,
  "quoteProps": "as-needed",
  "requirePragma": false,
  "useTabs": false
}
